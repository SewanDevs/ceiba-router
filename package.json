{
  "name": "gulp-restructure-tree",
  "version": "0.1.1",
  "description": "Restructure complex directory structures declaratively",
  "main": "index.js",
  "files": [
    "index.js",
    "yarn.lock"
  ],
  "scripts": {
    "build": "webpack --colors",
    "watch": "webpack --watch --colors",
    "test": "jest",
    "testWatch": "jest --watch",
    "coverage": "jest --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SewanDevs/gulp-restructure-tree.git"
  },
  "keywords": [
    "gulpplugin",
    "rename",
    "regex"
  ],
  "author": "lleaff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SewanDevs/gulp-restructure-tree/issues"
  },
  "homepage": "https://github.com/SewanDevs/gulp-restructure-tree#readme",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "gulp-util": "^3.0.7",
    "upath": "^0.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "coveralls": "^2.11.15",
    "jest": "^18.0.0",
    "webpack": "^1.14.0",
    "webpack-node-externals": "^1.5.4"
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(test|spec)\\.jsx?$",
    "rootDir": "src",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/__tests__/**",
      "!**/index.js",
      "!**/gulp-restructure-tree.js"
    ]
  }
}
